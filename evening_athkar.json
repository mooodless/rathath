[
    {
        "name": "آية الكرسي",
        "text": "اللَّهُ لَا إِلَٰهَ إِلَّا هُوَ الْحَيُّ الْقَيُّومُ لَا تَأْخُذُهُ سِنَةٌ وَلَا نَوْمٌ...",
        "text_dn": "من قالَها حين يُصبِح أُجير من الجن حتى يُمسي...",
        "text_total": "مرة واحدة"
    },
    {
        "name": "سورة الإخلاص",
        "text": "بسم الله الرحمن الرحيم (قُلْ هُوَ اللَّهُ أَحَدٌ...)",
        "text_dn": "من قال الإخلاص والمعوذات حين يُصبِح...",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "سورة الفلق",
        "text": "بسم الله الرحمن الرحيم (قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ...)",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "سورة الناس",
        "text": "بسم الله الرحمن الرحيم (قُلْ أَعُوذُ بِرَبِّ النَّاسِ...)",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "دعاء",
        "text": "أَمْسَيْـنا وَأَمْسـى المـلكُ لله وَالحَمدُ لله...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ بِكَ أَمْسَـينا وَبِكَ أَصْـبَحْنا...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "اللّهـمَّ أَنْتَ رَبِّـي لا إلهَ إلاّ أَنْتَ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ إِنِّـي أَمسيتُ أُشْـهِدُك...",
        "text_dn": "",
        "text_total": "أربع مرات"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ ما أَمسَى بِي مِـنْ نِعْـمَةٍ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ عافِـني في بَدَنـي...",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "دعاء",
        "text": "حَسْبِـيَ اللهُ لا إلهَ إلاّ هُوَ...",
        "text_dn": "",
        "text_total": "سبع مرات"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ إِنِّـي أسْـأَلُـكَ العَـفْوَ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "اللّهُـمَّ عالِـمَ الغَـيْبِ وَالشّـهادَةِ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "بِسـمِ اللهِ الذي لا يَضُـرُّ...",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "دعاء",
        "text": "رَضيـتُ بِاللهِ رَبَّـاً وَبِالإسلامِ...",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "دعاء",
        "text": "يَا حَيُّ يَا قيُّومُ بِرَحمَتِكَ أستَغِيثُ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "أَمْسَيْنا وَأَمْسَى الْمُلْكُ للهِ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "أَمْسَيْنَا عَلَى فِطْرَةِ الإسْلاَمِ...",
        "text_dn": "",
        "text_total": "مرة واحدة"
    },
    {
        "name": "دعاء",
        "text": "سُبْحَانَ الله وَبِحَمْدِهِ",
        "text_dn": "",
        "text_total": "مائة مرة"
    },
    {
        "name": "دعاء",
        "text": "لا إِلَهَ إِلاَّ اللهُ، وَحْدَهُ لاَ شَرِيكَ لَهُ...",
        "text_dn": "",
        "text_total": "عشر مرات"
    },
    {
        "name": "دعاء",
        "text": "أسْتَغْفِرُ اللهَ وَأتُوبُ إلَيهِ",
        "text_dn": "",
        "text_total": "مائة مرة"
    },
    {
        "name": "دعاء",
        "text": "أَعـوذُ بِكَلِمـاتِ اللّهِ التّـامّـاتِ...",
        "text_dn": "",
        "text_total": "ثلاث مرات"
    },
    {
        "name": "دعاء",
        "text": "اللَّهُمَّ صَلِّ وَسَلِّمْ عَلَى نَبَيِّنَا مُحَمَّدٍ",
        "text_dn": "",
        "text_total": "عشر مرات"
    }
]